with "apq.gpr";
with "apq-samples.gpr";
with "apq-mysql.gpr";

project APQ.MySQL.Samples is

	version := APQ.Version; 
	
	
	type Supported_OS is ("Windows_NT", "GNU/Linux");
	
	target: Supported_OS := external("OS", "GNU/Linux");
	
	
	
	for Source_Dirs use ( "src" );
	for Object_Dir use "obj";
	for Exec_Dir use "bin";

	for Main use ( "list_users", "add_user" );
	
	
	
	package Compiler is
		for Default_Switches ("ada") use ("-O2", "-gnat05");
	end Compiler;

end APQ.MySQL.Samples;

